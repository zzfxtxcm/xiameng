        .titleBox
          %p.active{style: "bottom: -40px;"}
            %span 第一张图片标题
          %p{style: "bottom: -40px;"} 第二张图片标题
          %p{style: "bottom: 0px;"} 第三张图片标
          %p{style: "bottom: -40px;"} 第四张图片标题
          %p 第五张图片标题
        .icoBox
